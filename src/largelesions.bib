  institution = {Moss Rehabilitation Research Institute, Albert Einstein Healthcare
	Network, Philadelphia, PA 19141, USA. kimj@einstein.edu},
  keywords = {Adolescent; Adult; Algorithms; Brain Injuries, pathology; Brain,
	pathology; Data Interpretation, Statistical; Diffusion Magnetic Resonance
	Imaging; Female; Humans; Image Processing, Computer-Assisted; Male;
	Middle Aged; Nonlinear Dynamics; Reference Values},
  language = {eng},
